{
    "name": "cosmos",
    "devDependencies": {
        "@types/bun": "latest"
    },
    "scripts": {
        "serve": "http-server -p 8081",
        "fatboyslim-css": "cp encrypted-storage/styles.css fatboyslim/dist/encrypted-storage.css",
        "fatboyslim-build": "npm run fatboyslim-css && ./build.sh fatboyslim",
        "fatboyslim-watch": "npm run fatboyslim-css && ./watch.sh fatboyslim",
        "fatboyslim-deploy": "az storage blob upload-batch -d '$web' --account-name earapps -s fatboyslim --destination-path fatboyslim --overwrite",
        "bdf": "npm run fatboyslim-build && npm run fatboyslim-deploy",
        "spendy-build": "./build.sh spendy",
        "spendy-watch": "./watch.sh spendy",
        "vault-build": "./build.sh vault",
        "vault-watch": "./watch.sh vault",
        "vault-deploy": "az storage blob upload-batch -d '$web' --account-name earapps -s vault --destination-path vault --overwrite",
        "bdv": "npm run vault-build && npm run vault-deploy"
    },
    "dependencies": {
        "@azure/storage-blob": "^12.24.0",
        "@tanstack/react-virtual": "^3.13.18",
        "@types/react": "^18.3.8",
        "@types/react-dom": "^18.3.0",
        "@types/underscore": "^1.11.15",
        "detect-libc": "^2.1.2",
        "exifr": "^7.1.3",
        "exiftool-vendored": "^35.2.0",
        "heic-convert": "^2.1.0",
        "http-server": "^14.1.1",
        "immer": "^10.1.1",
        "pdfjs-dist": "^3.11.174",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.26.2",
        "sharp": "^0.34.5",
        "typescript": "^5.7.3",
        "underscore": "^1.13.7"
    }
}
